# Production Environment Configuration
# This file is used when building for production

# API URL - Use relative path so nginx can proxy requests
REACT_APP_API_URL=/api

# Application Settings
PORT=3010
REACT_APP_DEFAULT_TIME_RANGE=1h
REACT_APP_THEME=light

# Feature Flags
REACT_APP_FEATURE_REPORTS=true
REACT_APP_FEATURE_ALERTS=true
REACT_APP_FEATURE_EMAIL=true

# Build optimization
GENERATE_SOURCEMAP=false
